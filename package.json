  "analyze": true,
  "license": "BSD-3-Clause",
  "preferGlobal": true,
  "author": "MyStand",
  "contributors": [{
    "name": "Stepan Kuzmin",
    "email": "to.stepan.kuzmin@gmail.com"
  }],
